{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend/main.py"
    }
  ],
  "env": {
    "PRODUCTION": "true",
    "PYTHONPATH": "backend"
  },
  "functions": {
    "backend/main.py": {
      "maxDuration": 30
    }
  }
}